{
  "proxies": {
    "permanent redirect": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/permanent"
      },
      "responseOverrides": {
        "response.statusCode": "301",
        "response.headers.Location": "https://docs.microsoft.com/"
      }
    },
    "root redirect": {
      "matchCondition": {
          "methods": [ "GET" ],
          "route": "/"
      },
      "backendUri": "http://localhost:7071"
  }
  }
}
